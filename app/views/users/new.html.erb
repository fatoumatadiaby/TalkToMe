<h1>Signup</h1>

<%= form_for @user, url: signup_path do |f| %>
 
 <div>
     <%= f.label :first_name%>
     <%= f.text_field :First_name%>
 </div>
 <div>
     <%= f.label :last_name%>
     <%= f.text_field :last_name%>
 </div>
 <div>
     <%= f.label :username%>
     <%= f.text_field :username%>
 </div>
 <div>
     <%= f.label :password%>
     <%= f.password_field :password%>
 </div>
      <%=f.submit "Signup"%>
 
 <%end %>


  